<div class="starter-template"><h1 class="sub-header">Listing reserves</h1></div>
<div class="table-responsive">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Date and Time</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>
      <% @events.each do |event| %>
        <tr>
          <td><%= event.name %></td>
          <td><%= event.created_for %></td>
          <td><%= link_to 'Show', users_team_event_path(@team, event), class: "btn btn-xs btn-info" %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <br>
  <%= link_to 'Back', users_team_path(@team), class: "btn btn-default" %>
</div>